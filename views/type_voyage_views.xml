<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="type_tree_view" model="ir.ui.view">
            <field name="name">type.tree.view</field>
            <field name="model">type_voyage</field>
            <field name="arch" type="xml">
                <tree string="type">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="type_form_view" model="ir.ui.view">
            <field name="name">type.form.view</field>
            <field name="model">type_voyage</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group  string="type de voyage">
                                <field name="name"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>

